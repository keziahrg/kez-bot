{
  "name": "kez-bot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate --no-engine --schema=./src/prisma/schema.prisma && next dev",
    "build": "prisma generate --no-engine --schema=./src/prisma/schema.prisma && prisma db push --schema=./src/prisma/schema.prisma && prisma db seed --schema=./src/prisma/schema.prisma && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "prisma": {
    "seed": "ts-node -r ./tsconfig-paths.js --compiler-options {\"module\":\"CommonJS\"} ./src/prisma/seed.ts"
  },
  "git": {
    "pre-commit": "npx lint-staged"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@prisma/extension-accelerate": "^0.6.2",
    "@vercel/analytics": "1.1.1",
    "@vercel/postgres": "0.5.1",
    "clsx": "2.0.0",
    "date-fns": "2.30.0",
    "eventsource-parser": "1.1.1",
    "next": "14.0.2",
    "next-themes": "0.2.1",
    "openai": "4.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.48.2",
    "react-intersection-observer": "9.5.3",
    "tailwind-merge": "2.0.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@hookform/resolvers": "3.3.2",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@vercel/git-hooks": "1.0.0",
    "autoprefixer": "10.4.13",
    "eslint": "8.53.0",
    "eslint-config-next": "14.0.2",
    "eslint-config-prettier": "9.0.0",
    "lint-staged": "15.0.2",
    "postcss": "8.4.31",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "0.5.7",
    "prisma": "5.7.0",
    "tailwindcss": "3.3.5",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2"
  }
}
